---
---
BEGIN:VCALENDAR
VERSION:2.0
PRODID:-//{{site.name}}//Events//EN
X-WR-CALNAME:{{site.name}} calendar
CALSCALE:GREGORIAN
{% for post in site.data.events %}BEGIN:VEVENT
DTSTART:{{ post.date | date: "%Y%m%dT%H%M%S" }}
DTEND:{{ post.end-date | date: "%Y%m%dT%H%M%S" }}
DTSTAMP:{{ post.date | date: "%Y%m%dT%H%M%S" }}
UID:events-{{ post.date | date: "%Y%m%d-%H%M" }}@hackcu.org
LOCATION:{{ post.location }}
URL:{{post.url}}
SUMMARY:{{ post.name }}
END:VEVENT
{% endfor %}END:VCALENDAR